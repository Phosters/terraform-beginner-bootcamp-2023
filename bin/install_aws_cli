#!/var/bin/env bash

cd /workspace

curl "https://awscli.amazonaws.com/awscli-exe-linux-x86_64.zip" -o "awscliv2.zip"
unzip awscliv2.zip
sudo ./aws/install
      
cd $PROJECT_ROOT

AWS_ACCESS_KEY_ID='******************'
AWS_SECRET_ACCESS_KEY='*********************'
AWS_DEFAULT_REGION='****************'

